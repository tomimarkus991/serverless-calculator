{
  "name": "react-webpack-babel-bulma-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for fast react development",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src config",
    "fix": "eslint --fix .",
    "start": "webpack-dev-server --config config/webpack.config.dev.js --open --hot",
    "serve": "serve -s build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomimarkus991/React-Webpack-Babel-Bulma-Boilerplate"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn fix"
    ],
    "tests/**/*.{js}": [
      "yarn fix"
    ],
    "config/**/*.{js}": [
      "yarn fix"
    ],
    "*/**/*.{js}": [
      "yarn fix"
    ],
    "src/**/*.{scss}": [
      "yarn fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "tomimarkus991",
  "license": "ISC",
  "homepage": "https://github.com/tomimarkus991/React-Webpack-Babel-Bulma-Boilerplate",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-polyfill": "6.26.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "file-loader": "^5.1.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-cli": "^6.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "sass-loader": "^8.0.2",
    "semantic-release": "^15.14.0",
    "serve": "^11.3.0",
    "shx": "^0.3.2",
    "style-loader": "^1.1.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "bulma": "^0.8.2",
    "eslint": "^6.8.0",
    "eslint-config-last": "0.0.5",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "prettier": "2.0.4"
  }
}